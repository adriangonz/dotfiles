execute pathogen#infect()

set backspace=2 
syntax on
filetype indent on
set autoindent
set number
set background=dark

colorscheme solarized

set nobackup

set tabstop=4
set softtabstop=4
set shiftwidth=4
set expandtab

noremap <Up> <NOP>
noremap <Down> <NOP>
noremap <Left> <NOP>
noremap <Right> <NOP>