{
  "python": {
    "linting": {
      "pylintEnabled": false,
      "flake8Enabled": true,
      "mypyEnabled": true
    }
  },
  "golang": {
    "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
    "command": "gopls",
    "filetypes": ["go"],
    "initializationOptions": {
      "usePlaceholders": true
    }
  }
}
